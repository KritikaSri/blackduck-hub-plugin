<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout" xmlns:t="/lib/hudson" xmlns:f="/lib/form">
	<!-- This jelly script is used for per-project configuration. See global.jelly 
		for a general discussion about jelly script. -->
		
	<st:once>
		<script type="text/javascript"
			src="${rootURL}/plugin/hub-jenkins/js/HubJenkinsWrapper.js" />
		<link href="${rootURL}/plugin/hub-jenkins/css/BdHubCustom.css"  rel="stylesheet" type="text/css"  />
	</st:once>
	
		
<f:section >
		<f:block>
			<table style="width:100%" id="ccBlackDuckTable">
				<f:entry title="${%MavenScopesField}" field="userScopesToInclude">
              		<f:textbox default="${%DefaultMavenScopes}" value="${it.userScopesToInclude}"/>
            	</f:entry>
            	
            	<f:entry field="sameAsPostBuildScan">
               		<!-- define checkbox first, label after for label to the right of the checkbox layout -->
               		<f:checkbox default="false" checked="${it.sameAsPostBuildScan}" onclick="useSameAsPostBuildScan(this, false);"/>
               		<label>${%SameAsPostBuildScanTitle}</label>
               		<div class="" id="sameAsPostBuildScanMessageArea"> </div>
            	</f:entry>
				
				<f:entry field="hubWrapperProjectName" title="${%ProjectNameTitle}">
					<f:textbox value="${it.getHubProjectName()}" default="${descriptor.getDefaultProjectName()}"/>
				</f:entry>

				<f:entry field="hubWrapperProjectVersion" title="${%ProjectVersionTitle}">
					<f:textbox value="${it.getHubProjectVersion()}" default="${descriptor.getDefaultProjectVersion()}"/>
				</f:entry>

				<f:entry field="hubWrapperVersionPhase" title="${%VersionPhaseTitle}">
					<f:select />
				</f:entry>
				<f:entry field="hubWrapperVersionDist" title="${%VersionDistTitle}">
					<f:select />
				</f:entry>
				
				<f:entry>
    				<div style="float:right">
      					<input name="hubCreateButton" type="button" value="${%CreateProject}" class="yui-button validate-button" onclick="customWrapperCreateProject('${descriptor.descriptorFullUrl}/createHubWrapperProject', 'hubWrapperProjectName,hubWrapperProjectVersion,hubWrapperVersionPhase,hubWrapperVersionDist', this);" />
    				</div>
    				<div style="display:none;">
      					<img src="${imagesURL}/spinner.gif" /> ${%CreateProjectProgress}
    				</div>
    				<div> </div>
				</f:entry>
            	
      </table>
    </f:block>
  </f:section>
		

       
</j:jelly>
